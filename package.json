{
  "name": "vue_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "postinstall": "yarn build"
  },
  "dependencies": {
    "@tonejs/midi": "^1.2.3",
    "axios": "^0.19.0",
    "core-js": "^2.5.7",
    "file-saver": "^2.0.1",
    "fuse.js": "^3.4.4",
    "opensheetmusicdisplay": "^0.7.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "tone": "^13.8.4",
    "vue": "^2.6.10",
    "vue-analytics": "^5.17.0",
    "vue-lodash": "^2.0.2",
    "vue-router": "^3.0.1",
    "vue-search-select": "^2.8.3",
    "vuetify": "^2.0.19",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "lint-staged": "^9.4.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
